  "scripts": {
    "build:assets": "gulp assets",
    "build:html": "gulp html",
    "build:js": "webpack",
    "build:styles": "gulp styles-production",
    "build": "cross-env NODE_ENV=production npm-run-all clean:temp build:assets build:styles build:js build:html rewrite",
    "clean:dist": "node _scripts/clean-dist",
    "clean:temp": "node _scripts/clean-temp",
    "rebuild": ". ./_scripts/rebuild.sh || true",
    "rewrite": "node _scripts/rewrite",
    "serve": "gulp serve",
    "start": "npm-run-all clean:* build:assets --parallel watch:*",
    "watch:gulp": "wait-on dist/js && gulp",
    "watch:js": "webpack --watch"
  },
